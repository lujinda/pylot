//登录框动态处理
function set_login_input(obj,id){
  if (id=="username"){
      if(obj.value=='') 
	    obj.style.backgroundImage='url(/images/login/loginbg1.gif)';
	  else 
	    obj.style.backgroundImage='none';
	 } 
	  
  if (id=="password"){
      if(obj.value=='')
	    obj.style.backgroundImage='url(/images/login/loginbg2.gif)';
	  else
	    obj.style.backgroundImage='none';
	}
	
}


